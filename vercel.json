{
  "version": 2,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node",
      "build": {
        "env": {
          "NODE_ENV": "production"
        },
        "command": "npm install --force"
      }
    }
  ],
  "routes": [
    
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}